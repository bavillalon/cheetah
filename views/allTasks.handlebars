<div class="container">
  <h2>All Tasks</h2>
  <p>These are all tasks including all unassigned and assigned ones</p>
  <p></p><a href="/teacher" style="text-decoration-line: underline">Back to Teacher View</a></p>            
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Task</th>
        <th>Description</th>
        <th>Quantity</th>
        <th>Due Date</th>
        <th>Estimated Time</th>
        <th>State</th>
      </tr>
    </thead>
    <tbody>
    {{#each tasks}}
      <tr>
        <td>{{task_name}}</td>
        <td>{{description}}</td>
        <td>{{quantity}}</td>
        <td>{{dueDate}}</td>
        <td>{{estimatedTime}}</td>
        <td>{{state}}</td>
        </tr>
  {{/each}}
    </tbody>
  </table>
</div>
</body>

